<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="/pretty-svn-index.xsl"?>
<!DOCTYPE svn [
    <!ELEMENT svn   (index)>
    <!ATTLIST svn   version CDATA #REQUIRED
        href    CDATA #REQUIRED>
    <!ELEMENT index (updir?, (file | dir)*)>
    <!ATTLIST index name    CDATA #IMPLIED
        path    CDATA #IMPLIED
        rev     CDATA #IMPLIED
        base    CDATA #IMPLIED>
    <!ELEMENT updir EMPTY>
    <!ATTLIST updir href    CDATA #REQUIRED>
    <!ELEMENT file  EMPTY>
    <!ATTLIST file  name    CDATA #REQUIRED
        href    CDATA #REQUIRED>
    <!ELEMENT dir   EMPTY>
    <!ATTLIST dir   name    CDATA #REQUIRED
        href    CDATA #REQUIRED>
    ]>
<svn version="1.13.0 (r1867053)"
     href="http://subversion.apache.org/">
  <index rev="0" path="Collection of Repositories">
    <dir name="repo-a" href="repo-a/"/>
    <dir name="repo-b" href="repo-b/"/>
  </index>
</svn>
