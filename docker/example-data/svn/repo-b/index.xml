<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="/pretty-svn-index.xsl"?>
<!DOCTYPE svn [
    <!ELEMENT svn   (index)>
    <!ATTLIST svn   version CDATA #REQUIRED
        href    CDATA #REQUIRED>
    <!ELEMENT index (updir?, (file | dir)*)>
    <!ATTLIST index name    CDATA #IMPLIED
        path    CDATA #IMPLIED
        rev     CDATA #IMPLIED
        base    CDATA #IMPLIED>
    <!ELEMENT updir EMPTY>
    <!ATTLIST updir href    CDATA #REQUIRED>
    <!ELEMENT file  EMPTY>
    <!ATTLIST file  name    CDATA #REQUIRED
        href    CDATA #REQUIRED>
    <!ELEMENT dir   EMPTY>
    <!ATTLIST dir   name    CDATA #REQUIRED
        href    CDATA #REQUIRED>
    ]>
<svn version="1.13.0 (r1867053)"
     href="http://subversion.apache.org/">
  <index name="Pretty SVN Index" rev="42" path="/repo-b" base="repo-a">
    <updir href="../"/>
    <dir name="demo-project" href="demo-project/"/>
    <dir name="index-without-fields" href="index-without-fields/"/>
    <dir name="no-base" href="no-base/"/>
    <dir name="no-name" href="no-name/"/>
    <dir name="no-path" href="no-path/"/>
    <dir name="no-revision" href="no-revision/"/>
    <dir name="overflowing-stuff" href="overflowing-stuff/"/>
  </index>
</svn>

